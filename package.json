{
  "name": "portfolio",
  "version": "1.0.0",
  "private": true,
  "description": "A front-end site to house my portfolio",
  "homepage": "https://www.danielmaddison.io",
  "bugs": "https://github.com/DanMad/portfolio/issues",
  "repository": {
