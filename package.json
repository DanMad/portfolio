    "sass-loader": "^12.3.0",
    "sort-package-json": "^1.53.1",
    "style-loader": "^3.3.1",
    "webpack": "^5.64.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0"
  }
